{
  "name": "bustle",
  "version": "1.1.0",
  "productName": "Bustle Spot",
  "description": "Time Manangment App",
  "author": "Softsuave, Inc <softsuave@gmail.com>",
  "private": true,
  "homepage": "./",
  "files": [
    "*.js",
    "build"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "active-win": "^6.3.0",
    "axios": "^0.19.2",
    "electron-is-dev": "^1.2.0",
    "iohook": "^0.6.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-google-login": "^5.1.10",
    "react-hook-form": "^7.31.3",
    "react-query": "^3.39.1",
    "react-redux": "^ 7.2.6",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "react-start": " set PORT=4000 && react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "yarn run react-build && yarn run electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn run react-start\" \"wait-on http://localhost:4000 && electron .\"",
    "flow": "flow",
    "flow-start": "flow start",
    "flow-stop": "flow stop",
    "eslint": "eslint --fix --ext .jsx --ext .js src/**/*.js",
    "prettierx:all": "prettier --write src/**/*{.ts,.tsx}",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "food": "tsc"
  },
  "main": "./public/electron.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-flow": "^7.8.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "electron": "^7.3.3",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.11.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "flow-bin": "^0.116.1",
    "prettier": "^2.6.2",
    "react-scripts": "^3.4.4",
    "redux-logger": "^3.0.6",
    "wait-on": "^3.3.0"
  },
  "iohook": {
    "targets": [
      "node-64",
      "electron-75"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "build": {
    "appId": "com.softsuave",
    "compression": "normal",
    "productName": "BustleSpot",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "mac": {
      "icon": "src/assets/icons/Bustle.icns",
      "type": "distribution",
      "target": [
        "pkg",
        "dmg",
        "mas"
      ],
      "category": "public.app-category.news"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/Bustle.ico"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "description": "Time Manangment App",
      "category": "Productivity"
    }
  }
}
